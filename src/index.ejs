            </div>
            <table id="mainTable">
                <% for(var i = 0; i < 10; i++) { %>
                <tr>
                    <th></th>
                    <% for(var j = 0; j < 9; j++) { %>
                    <td></td>
                    <% } %>
                </tr>
                <% } %>
            </table>
        </div>
    </body>
</html>